#!/bin/bash
if test "${1}" == "touch"  
then
    shift
    echo "<title> $@ </title>" > "${HOME}/.local/var/browsertitle/$@.html"
    firefox "file://$(realpath ${HOME}/.local/var/browsertitle/$@.html)"
elif test "${1}" == "rm"
then
    shift
    rm "${HOME}/.local/var/browsertitle/$@.html"
else
	echo "Usage: $(basename ${0}) {touch|rm}"
fi
